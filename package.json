{
  "name": "chatr",
  "version": "0.1.17",
  "private": true,
  "scripts": {
    "dev": "set HTTPS=true&&set SSL_CRT_FILE=cert.pem&&set SSL_KEY_FILE=key.pem&&react-scripts start",
    "lint": "eslint \"{src,apps}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" ",
    "start": "electron-forge start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "ionic serve --https",
    "android:build": "ionic capacitor build android --prod",
    "ios:build": "ionic capacitor build ios --prod",
    "desktop:serve": "npm run build && npm run desktop:build && cd electron && npm run electron:start",
    "desktop:build": "cap sync @capacitor-community/electron",
    "desktop:build:mac": "cd electron && npm run electron:build:mac",
    "desktop:build:windows": "cd electron && npm run electron:build:windows",
    "desktop:build:linux": "cd electron && npm run electron:build:linux"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^3.6.0",
    "@capacitor/core": "^3.6.0",
    "@capacitor/storage": "^1.2.5",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/social-sharing": "^5.36.0",
    "@ionic/core": "^6.1.13",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/react": "^6.1.13",
    "@ionic/react-router": "^6.1.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/howler": "^2.2.7",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/qs": "^6.9.7",
    "@types/quill": "^2.0.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^0.27.2",
    "copy-to-clipboard": "^3.3.1",
    "cordova-plugin-x-socialsharing": "^6.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "es6-promise-plugin": "^4.2.2",
    "fabric": "^5.2.1",
    "faye": "^1.4.0",
    "howler": "^2.2.3",
    "ionicons": "^6.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nanoid": "^4.0.0",
    "peerjs": "^1.4.6",
    "prop-types": "^15.8.1",
    "qs": "^6.11.0",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^3.6.0",
    "@capacitor/ios": "^3.6.0",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
